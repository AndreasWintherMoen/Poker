   <children>
      <ImageView fx:id="background" fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" />
      <VBox minHeight="720.0" minWidth="1280.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox alignment="BOTTOM_CENTER" minWidth="1280.0" prefHeight="160.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="opponentFirstCard" fitHeight="150.0" fitWidth="95.0" pickOnBounds="true" preserveRatio="true" translateX="-10.0" />
                  <ImageView fx:id="opponentSecondCard" fitHeight="150.0" fitWidth="95.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" translateX="10.0" />
               </children>
            </HBox>
            <HBox minHeight="400.0" minWidth="1280.0" prefHeight="100.0" prefWidth="200.0" />
            <HBox alignment="TOP_CENTER" layoutX="10.0" layoutY="10.0" minWidth="1280.0" prefHeight="160.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="playerFirstCard" fitHeight="150.0" fitWidth="95.0" pickOnBounds="true" preserveRatio="true" translateX="-10.0" />
                  <ImageView fx:id="playerSecondCard" fitHeight="150.0" fitWidth="95.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" translateX="10.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
